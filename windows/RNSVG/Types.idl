  };

  [default_interface]
  runtimeclass D2DDeviceContext {
    D2DDeviceContext();
  };

  [default_interface]
  runtimeclass D2DGeometry {
    D2DGeometry();
  };

  [default_interface]
  runtimeclass D2DBrush {
    D2DBrush();
  };
